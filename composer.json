{
  "name": "moon-php/autoloader",
  "description": "A very simple Autoloader with support for PSR4 and PSR0.",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "authors": [
    {
      "name": "Damiano Petrungaro",
      "email": "damiano.petrungaro@qwentes.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "Moon\\Autoloader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Moon\\Autoloader\\Unit\\": "tests/Unit/"
    }
  }
}
